{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "AddressInputBatch",
  "type": "object",
  "required": ["idempotency_key", "batch_name", "ruleset_id", "records"],
  "properties": {
    "idempotency_key": {
      "type": "string",
      "minLength": 6,
      "maxLength": 128
    },
    "batch_name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "ruleset_id": {
      "type": "string",
      "minLength": 1,
      "maxLength": 64
    },
    "records": {
      "type": "array",
      "minItems": 1,
      "maxItems": 2000,
      "items": {
        "type": "object",
        "required": ["raw_id", "raw_text"],
        "properties": {
          "raw_id": {"type": "string", "minLength": 1, "maxLength": 64},
          "raw_text": {"type": "string", "minLength": 1, "maxLength": 1024},
          "province": {"type": "string", "maxLength": 64},
          "city": {"type": "string", "maxLength": 64},
          "district": {"type": "string", "maxLength": 64},
          "street": {"type": "string", "maxLength": 128},
          "detail": {"type": "string", "maxLength": 1024}
        },
        "additionalProperties": false
      }
    }
  },
  "additionalProperties": false
}
