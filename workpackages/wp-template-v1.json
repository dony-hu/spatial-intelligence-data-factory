  },
  "engine_bundle": {
    "runtime": "factory_engine",
    "mode": "single_record"
  },
  "observability_bundle": {
    "generator": "factory_observability_generator",
    "entrypoints": ["line_observe.py", "line_metrics.json"]
  },
  "rollback": {
    "from_version": "1.0.0",
    "fallback_version": "0.9.0",
    "instructions": [
