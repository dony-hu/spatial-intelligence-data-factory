{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "POI Fixture Schema",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["poi_id", "name", "lat", "lng", "category", "source"],
    "properties": {
      "poi_id": { "type": "string" },
      "name": { "type": "string" },
      "lat": { "type": "number" },
      "lng": { "type": "number" },
      "category": { "type": "string" },
      "source": { "type": "string", "enum": ["synthetic", "masked"] }
    },
    "additionalProperties": false
  }
}
